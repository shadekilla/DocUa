+modal("select-area", "bs-modal-md")
    .modal-header
        button.close.close__select(aria-hidden="true", data-dismiss="modal", type="button")
        .modal-title
            | Место приема врача
        ul.nav.nav-pills.inline-block
            li Выберите нажатием
            li.active
                a(href="#select-area-districts", data-toggle="tab")
                    span районы
            if file.page_name != 'clinic-profile_current'     
                li &nbsp;или&nbsp;
                li
                    a(href="#select-area-metro", data-toggle="tab")
                        span станции метро
    .modal-body 
        .tab-content.clear
            - var selectAreaClasses = ''
            - var metroMapClasses = ''
            - var districtsList = districts
            - var metroList = metro
            if location == 'kharkiv'
                - districtsList = districtsKharkiv
                - metroList = metroKharkiv
                - selectAreaClasses = 'select-area_location_kharkiv'
                - metroMapClasses = 'metro-map_kharkiv' 
            #select-area-districts.tab-pane.active.js-list-scroll(class=selectAreaClasses)
                ul.districts-check-list.district-list__mod.columnize(data-columns="5", data-min-column-width="0", data-adaptive-column-count="true")
                    each district in districtsList
                        li.pill-group
                            label.pill.with-icon
                                input(type="checkbox")
                                span: span= district.title
                            if district.children
                                ul
                                    each subDistrict in district.children
                                        li
                                            label.pill.with-icon
                                                input(type="checkbox")
                                                span: span= subDistrict
            #select-area-metro.tab-pane(class=selectAreaClasses)
                .metro-map-min.metro-map(class=metroMapClasses)
                    each station in metroList
                        label.pill.with-icon(class=station.class, style="left: #{station.x}px; top: #{station.y}px;")
                            input(type="checkbox")
