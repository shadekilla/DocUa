.form-radio
    width 283px
    float right 
    font-size 14px 
.profile__item
    .modal-radio
        display none
    .modal-radio + label
        margin-left 28px
        padding-right 30px
        position relative
        cursor pointer
        &:before
            content ''
            display inline-block
            background url('i/cabinet/checkbox.png') no-repeat
            position absolute
            left -30px
