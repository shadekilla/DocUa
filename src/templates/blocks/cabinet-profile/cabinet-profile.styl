//cabinet
.cabinet-aside
    border 1px solid rgba(0, 130, 155, 0.5)
    border-radius 3px
    background-color white  
    box-shadow 0 2px 4px rgba(0, 64, 76, 0.19)
    &__item
        position relative
        font-family 'PT Sans', sans-serif
        font-size 16px
        font-weight 400
        line-height 12px
        text-transform uppercase
        padding 20px 0 14px 60px
        border-bottom 1px dotted rgba(#1d89a0, 0.45)
        &:hover
            color #00829b    
        &:last-child
            border-bottom 0
        &.is-active
            color rgba(#00829b, 0.94)
            font-weight 700
            &:after, &:before
                content ""
                border 8px solid transparent
                position absolute
                top 15px
                right -18px
                margin-left -12px
                width 0 
            &:before 
                border-left-color rgba(0, 130, 155, 0.8)
                border-width 9px
            &:after
                border-left-color white
                border-width 9px
                right -17px
        .qt-note
            color #91b5bc
            font-size 16px
            padding-left 20px 
            font-weight 700 
    &__link
        text-decoration none
        &-profile:after
            content ''
            position absolute
            left 30px
            top 14px
            display inline-block 
            width 17px
            height 19px
            background url('i/cabinet/profile-icon.png') no-repeat center left
            transition all 0.3s
        &-profile:hover:after
                background url('i/cabinet/profile-icon-hover.png') no-repeat center left
        &-profile.is-active:after
                background url('i/cabinet/profile-icon-hover.png') no-repeat center left 
        &-note:after
            content ''
            position absolute
            left 28px
            top 14px
            display inline-block
            width 19px 
            height 19px
            background url('i/cabinet/pencil-icon.png') no-repeat center left
        &-note:hover:after
                background url('i/cabinet/pencil-icon-hover.png') no-repeat center left
        &-note.is-active:after
                background url('i/cabinet/pencil-icon-hover.png') no-repeat center left
                
        &-reviews:after
            content ''
            position absolute
            left 28px
            top 14px
            display inline-block
            width 19px
            height 20px
            background url('i/cabinet/star-icon.png') no-repeat
        &-reviews:hover:after
                background url('i/cabinet/star-icon-hover.png') no-repeat center left
        &-reviews.is-active:after
                background url('i/cabinet/star-icon-hover.png') no-repeat center left  
        &-like:after 
            content ''
            position absolute
            left 28px
            top 17px
            display inline-block
            width 19px
            height 16px
            background url('i/cabinet/like-icon.png') no-repeat
        &-like:hover:after
                background url('i/cabinet/like-icon-hover.png') no-repeat center left
        &-like.is-active:after
                background url('i/cabinet/like-icon-hover.png') no-repeat center left

//profile
.cabinet-section 
    margin-bottom 40px
    min-width 960px
    &__header
        border-radius 3px 3px 0 0
        min-height 40px
        background-color #1d89a0
        color #FFF
        font-size 15px
        line-height 40px
        padding-left 30px
    &__content
        box-shadow 0 2px 4px rgba(0, 64, 76, 0.19)
        border 1px solid rgba(0, 130, 155, 0.5)
        border-radius 0 0 3px 3px 
        padding 30px 0 25px 0 
        background-color #FFF 
    .is-filled
        input
            background-color #edf6f7   
    .profile
        max-width 593px
        margin 0 auto
        padding-right 110px 
        font-size 0
        &__item
            position relative 
            margin-bottom 16px
            &:last-child
                margin-bottom 0 
            &.item__mod
                display none
            .modal-radio
                +label
                    &:hover
                        color text_color
        &__title
            display inline-block
            width 200px
            font-size 15px
            color rgba(#5e6e71, 0.94)
            line-height 30px
        &__field-text
            display inline-block
            width 283px
            height 33px
            background-color white
            border 1px solid rgba(22, 141, 164, 0.7)
            border-radius 3px
            color #91b5bc
            font-size 15px
            font-weight 700 
            padding 0 30px 0 15px
        &__field-select
            padding 0 0 0 15px 
        &__field-icon
            position absolute
            right 7px
            top 6px
            display inline-block
            width 20px
            height 20px
            cursor pointer 
            background url('i/cabinet/piker-calendar.png') no-repeat
        .field-text__child 
            position relative 
            padding 0 0 0 20px
            line-height 30px
            margin 0
            &:after
                content ''
                float right
                margin 12px 6px 0 0 
                display inline-block
                border 3px solid transparent
                border-top 6px solid #1d89a0 
                
        .section-choice__city
            height 177px 
            overflow hidden
            outline none
            padding 10px 0  
            .section-choice__item
                margin-right -10px  
            .jspContainer
                .jspPane
                    margin 0       
                .jspVerticalBar  
                    right 4px 
                    left auto  
                .jspDrag
                    background-color #a9d1da
                    border-radius 2px
                    width 5px 
                    right 2px  
                    left auto
                    margin-bottom 5px      
                .jspTrack
                    width 1px
                    background-color #a9d1da  
        .section-choice
            display none
            position absolute
            top 32px 
            z-index 2
            background-color #FFF  
            right 0
            width 283px
            border 1px solid rgba(22, 141, 164, 0.7)
            -webkit-border-radius 0 0 3px 3px  
            border-radius 0 0 3px 3px 
            color #91b5bc
            
            .section-choice__item 
                cursor pointer
                font-size 14px 
                padding 5px 10px 5px 20px  
                color #5e6e71
                &:hover
                    background-color #e5f2f5 
                    color #168da4
        .js-group
            display none
            &:after
                content ''
                display: table
                clear: both
            *zoom: 1 
        .field-text__in
            position relative
            float right
            &:after
                content ''
                display: table
                clear: both
            *zoom: 1
        .field-text__group
            float right
            &:after
                content ''
                display: table
                clear: both
            *zoom: 1
        .field-text__mod
            margin-top 15px
        .is-open
            display block 
        .field-group
            margin-bottom 15px
        .piker-calendar 
            position absolute
            right 7px
            top 6px
            display inline-block
            width 20px
            height 20px
            cursor pointer 
            background url('i/cabinet/piker-calendar.png') no-repeat
        .profile-parol
            position absolute
            right -138px
            top 5px
            font-size 14px
            @media only screen and (max-width: 1150px) 
                font-size 11px
                right -105px 
                
            &__link
                color #4b89d3
                font-style italic
            &__icon
                display inline-block
                width 14px
                height 14px
                margin 0 5px -3px 0
                background url('i/cabinet/parol.png') no-repeat 
        .profile-tel
            right -158px
            @media only screen and (max-width: 1150px)  
                font-size 11px
                right -118px
            .profile-tel__icon
                display inline-block
                width 10px
                height 16px
                margin 0 6px -3px 0
                background url('i/cabinet/tel.png') no-repeat  
        .add-tel
            display none
            margin-top 15px
            transition all 0.5s  
            &.is-active
                display inline-block
        .add-tel__mod
            margin-left 200px   
        .btn-add-tel
            position relative
            float right
            width 283px
            height 33px
            background-color #1d89a0
            border-radius 3px
            color white
            font-size 15px
            font-weight 700
            line-height 33px
            cursor pointer
            padding-left 65px
            &:hover
                background-color #1b7f94
            span
                position absolute
                display inline-block
                left 34px
                top 6px
                width 21px
                height 21px
                background url('i/cabinet/add-tel.png') no-repeat
        
        .item-in__right
            float right
            // margin-left 200px
        .item-in__left
            float left
            width auto
        .child-field
            width 130px
            margin-left 22px
            &:nth-child(even)
                margin-left 0
        .child-calendar__mod
            right  159px 

            
    .profile-footer
        max-width 593px
        margin 0 auto
        padding-right 110px 
        &:after
            content ''
            display: table
            clear: both 
        
        
        &__btn
            display inline-block
            width 283px
            height 45px
            background-color green_base
            border-radius 3px
            text-decoration none
            text-align center
            line-height 45px
