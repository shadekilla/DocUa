    .cabinet-notes 
        width 100% 
        // min-width 1200px 
        // width 1000px
        outline none 
        margin-bottom 20px     
        box-shadow 0 2px 4px rgba(0, 64, 76, 0.19) 
        border-radius 3px 
        moz-border-radius 3px
        -webkit-border-radius 3px
        border-collapse separate 
        &__head
            tr
                height 42px
                background-color #1d89a0
                border 1px solid #1d89a0 
                color #FFF
                font-size 15px
                // line-height 12px   
                td
                    &:first-child
                        background-color #1d89a0
                        border-right 1px solid #1d89a0 
                        padding 0  
                        text-align right
                        border-top-left-radius 3px
                        moz-border-top-left-radius 3px
                        -webkit-border-top-left-radius 3px
                    &:nth-child(2)
                        text-align center 
                    &:last-child
                        background-color #1d89a0
                        border-left 1px solid #1d89a0  
                        border-top-right-radius 3px
                        moz-border-top-right-radius 3px
                        -webkit-border-top-right-radius 3px 
                          
        &__body
            font-size 15px
            border 1px solid rgba(0, 130, 155, 0.32) 
            tr
                height 65px 
                &:nth-child(even)
                    background-color rgba(245, 250, 252, 0.48)
                &:nth-child(odd)
                    background-color white
                &:last-child
                    td    
                        &:first-child
                            border-bottom 1px solid rgba(0, 130, 155, 0.32)
                            border-bottom-left-radius 3px
                            moz-border-bottom-left-radius 3px
                            -webkit-border-bottom-left-radius 3px 
                        &:nth-child(2)
                            border-bottom 1px solid rgba(0, 130, 155, 0.32)   
                        &:nth-child(3) 
                            border-bottom 1px solid rgba(0, 130, 155, 0.32)
                        &:nth-child(4)
                            border-bottom 1px solid rgba(0, 130, 155, 0.32)
                        &:last-child 
                            border-bottom 1px solid rgba(0, 130, 155, 0.32) 
                            border-bottom-right-radius 3px
                            moz-border-bottom-right-radius 3px
                            -webkit-border-bottom-right-radius 3px   
                td 
                    &:first-child
                        width 10%
                        text-align right
                        border-bottom 1px dotted rgba(#1d89a0, 0.45)
                        border-left 1px solid rgba(0, 130, 155, 0.32) 
                          
                    &:nth-child(2)
                        text-align center
                        border-bottom 1px dotted rgba(#1d89a0, 0.45)  
                    &:nth-child(3)
                        width 20%
                        border-bottom 1px dotted rgba(#1d89a0, 0.45)
                    &:nth-child(4)
                        width 25%
                        border-bottom 1px dotted rgba(#1d89a0, 0.45)
                    &:last-child 
                        width 190px
                        border-bottom 1px dotted rgba(#1d89a0, 0.45)
                        border-right 1px solid rgba(0, 130, 155, 0.32)  
                    a 
                       color #4b89d3
                    span
                        font-size 14px
                        font-style italic 
                    .btn-edit
                        background-color green_base
                        border-radius 3px 
                        padding 10px 15px
                        text-shadow 0.5px 0.9px 0 rgba(48, 49, 49, 0.08)
                        color white
                        font-weight 700
                        text-decoration none
                        &:hover
                            background-color #33b3a4
                        
