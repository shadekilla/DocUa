$finder_input_padding = 8px
.finder
    margin-top 20px
    margin-bottom 45px  
    padding 0 10px 
    position relative
    z-index 2
    white-space nowrap
    clearfix()
    @media phone
        margin-bottom 20px 
    &.finder_main-page
        max-width 1150px 
        width 100% 
        // margin 20px auto 45px
        padding 0 
    &.search-title_none
        .finder__search-title
            display none
    
    &__search-title
        position relative
        float left
        width 12%
        height 78px
        padding 25px 0
        color #fff
        font-weight 700
        letter-spacing 0.05em  
        font-size 14px 
        line-height 28px
        text-transform uppercase
        text-align center
        background-color green_base
        border-radius border_radius_sm 0 0 border_radius_sm
        
        @media phone_tablet
            display none
        
        &:after
            content ""
            position absolute
            right -19px
            top 0
            width 0
            height 0
            border-style solid
            border-width 39px 0 39px 20px
            border-color transparent transparent transparent green_base
    
    form
        position relative 
        float left
        width 100%
        padding 0 
        @media phone_tablet
            width 100%
        &:before, &:after
            display none
            // content ""
            // position absolute
            // top 0
            // left 0
            // bottom 0
            // right 0  
            // width 100% 
            // background-color rgba(255, 255, 255, 0.18)
            // opacity 0.29
            // border 1px dashed white
            // border-radius 2px

        fieldset
            position relative
    
    
    &__field
        position relative
        float left
        // display inline-block
        margin-right 2px
        width 28%
        height 42px
        color #5e6e71
        font-size 16px
        line-height 42px
        box-shadow 0 0 20px transparent
        transition box-shadow .2s ease-in-out
        background-color #fff
        white-space nowrap
        cursor pointer
        &:hover
            box-shadow none
        &.finder-field-single
            &:nth-child(2)
                width 67.5%
            @media phone
                &:nth-child(2)
                    width 100%
        &.finder__field-icon-doctors
            border-radius: 0 0 0 0        
        @media phone
            &:first-child
                width 100%
                -webkit-border-radius 2px 
                border-radius 2px 
            &:nth-child(2)
                width 100%
            &:nth-child(3)
                width 100%
        &:first-child
            border-radius 2px 0 0 2px
            // width 17.7%
            width 17.1%
            // width 196px 
        &:nth-child(2)
            // width 42.4%
            width 41.3%
            // width 474px
        &:nth-child(3)
            width 26% 
            // width 299px
               
        input.form-control
            // width 100%
            height 42px 
            // padding-left 0
            font-size 16px
            border 0
            box-shadow none
            padding 6px 65px 6px 20px
            color #5e6e71
            background transparent
            // padding 0 45px 0 18px
            &:focus
                &::placeholder
                    font-size 0
            &::placeholder 
                color #5e6e71 !important
                opacity 1 
                -ms-text-overflow ellipsis !important
                text-overflow ellipsis !important
            @media phone 
                font-size 14px
                height auto

        // @media tablet
        //     width 39%
        @media phone
            width auto
            float none
            display block
            margin 0 0 20px
            -webkit-border-radius 2px 
            border-radius 2px 
        // &:before
        //     content ""
        //     position absolute
        //     transition opacity .2s ease-in-out
        //     background-repeat no-repeat
        //     width 35px
        // &:before
        //     height 27px
        //     right $finder_input_padding
        //     top 6px
        // &:hover
        //     box-shadow 0 0 20px rgba(53, 191, 174, 1)
        //     .finder__field_img
        //         opacity .75
        .finder__right-block
            position absolute
            z-index 1
            right 0
            top 5px
            bottom 0
            // min-width 28px
            height 32px
            border-left 1px solid #d9ecf0
            text-align center 
            background #FFF 
            transition opacity .2s ease
            &:hover
                opacity 0.5
            @media phone
                width: 62px
            i.triagle
                display inline-block
                vertical-align super
                width 0
                height 0
                border-left 4px solid transparent
                border-right 4px solid transparent
                border-top 4px solid #00829b
                margin 0 10px 2px 10px 
        &_img
            // position absolute
            // top 50%
            // right 50% 
            // transition opacity .2s ease-in-out
            // background-repeat no-repeat
            position relative
            top auto
            right auto
            display inline-block
            margin-right 10px
        &-icon
            &-doctors
                .finder__right-block
                    @media phone
                        padding-left 20px
                    >span
                        background-image url('i/new-search/icon-list-doctors.png') 
                        // margin-bottom  2px 
                        width 23px
                        height 24px
                    .triagle
                        @media phone
                            position absolute
                            left 0
                            top 50%
                            margin-top -1px
            &-location
                .finder__right-block
                    @media phone
                        padding-left 20px
                    >span
                        background-image url('i/new-search/search-location.png') 
                        margin-bottom  2px 
                        width 13px
                        height 18px
                    .triagle
                        @media phone
                            position absolute
                            left 0
                            top 50%
                            margin-top -1px
            // &-clinics
            //     >span
            //         right $finder_input_padding
            //         top 12px
            //         sprite($finder_clinic)
            // &-aim
            //     >span
            //         right 4px
            //         top 8px
            //         sprite($finder_aim)
            // &-cla
            //     >span
            //         right 12px
            //         top 12px
            //         background url('i/cla/cla-finder.png')
            //         width 20px
            //         height 26px 
                
        &-text
            display inline-block
            color form_input_color
            overflow hidden
            text-overflow ellipsis
            white-space nowrap
            &:focus
                text-overflow ellipsis !important
            // width auto
            // @media phone_tablet
            //     width 100%
                
        .select-type 
            font-weight bold
            color #5e6e71
            .select7__current
                padding 0 30px 0 20px 
                overflow hidden
                text-overflow ellipsis
                white-space nowrap
            .select7__current-value
                line-height normal
                vertical-align text-bottom
                overflow hidden
                text-overflow ellipsis
                white-space nowrap
                width 100%
                
                @media phone
                    font-size 14px
                &:hover
                    color #5e6e71 !important
                
            .select7__drop
                right 0
                border-radius 0 0 0 0
                max-height 168px
                overflow-y auto
                max-width auto
                .select7__drop-list
                    
                    .select-type__item
                        font-size 16px
                        color #5e6e71 
                        border-top 1px solid #d9ecf0
                        font-weight 400
                        line-height 40px
                        padding 0 20px 
                        white-space nowrap
                        text-overflow ellipsis
                        overflow hidden
                        transition background .2s ease
                        &:hover
                            // background #deeff2
                            background #d9ecf0
        .select7
            position absolute
            top 0
            left 0
            height 100%
            width 100%
            margin 0
            z-index 2
            &:hover
                & + .finder__right-block
                    opacity 0.5
            &__current
                padding 2px 45px 0 18px
                &-value
                    border 0 none
            &__caret
                display none
            &__placeholder
                color #d5d5d5
            &__drop
                border 0 none
                padding 5px 0 0 0
                margin -5px 0 0 0
                border-radius 0 0 border_radius_sm border_radius_sm
                overflow hidden
                z-index 10001
            &__option
                font-size 18px
                line-height 38px
                padding 0 18px
                &:hover
                    background-color #d7f2ef
        .ui-autocomplete
            position absolute
            z-index 3
            // width 410px
            width calc(100% - 1px)
            // height 100px 
            background-color white
            border 1px solid #d9ecf0
            border-radius 2px
            box-shadow 0 3px 8px rgba(0, 0, 0, 0.02)
            white-space normal
            line-height normal 
            // padding 17px 0
           
            .ui-menu-item
                position relative
                margin -1px 18px 0 18px 
                border-bottom 1px solid #d9ecf0
                padding 0
                &:before
                    content ''
                    position absolute
                    z-index 2 
                    left -18px
                    top 0
                    width 18px
                    height 100%
                    background-color transparent 
                &:last-child
                    border-bottom 1px solid transparent
                &:hover
                    margin-top -1px
                    background-color #eef9fb
                    border-bottom 1px solid #eef9fb
                    &:before
                        background-color #eef9fb
                &.js-text-hidden
                    max-height 69px 
                &.no-scroll
                    &:after
                        content ''
                        position absolute
                        z-index 2 
                        right -18px
                        top 0
                        width 18px
                        height 100%
                        background-color transparent 
                    &:hover
                        &:after
                            background-color #eef9fb 
                a
                    display inline-block
                    padding 15px 0px 15px 0px 
                    color #7c888a
                    text-decoration none
                    font-size 15px
                span.is-active
                    color #00829b
                    font-weight 700
        .icon-clean
            display none
            position absolute
            top 54%
            right 65px
            transform: translateY(-50%)
            background-image: url('i/icons/action-close.png')
            width 12px
            height 12px
            cursor pointer 
            &:hover
                opacity: 0.8 
    &__submit
        display inline-block 
        width 15% 
        
        // @media tablet
        //     width 21.5%
        @media phone
            width 100%
        .btn
            height 42px
            font-size 20px
            padding 0
            font-weight 700
            text-shadow 0 1px 1px rgba(180, 137, 63, 0.75)
            background-color #fd8f2e            
            outline none !important
            box-shadow none 
            border-radius 0 2px 2px 0
            border-color #fd8f2e 
            @media phone  
                width 100%
                -webkit-border-radius 2px 
                border-radius 2px 
            @media only screen and (min-width: 992px) and (max-width: 1100px)
                font-size 19px 
            &:hover
                background-color #fda82e
            &:active, &:focus
                background-color #fda82e
    .nav-pills
        margin 0
        >li
            margin -6px 0 0 5px
            &:first-child
                margin-left 0
            >a
                background-color transparent
                color #0089d1
                border-radius border_radius_sm
                border 0 none
                padding 0 10px
                font-size 16px
                line-height 30px
                &:hover
                    background none
                span
                    border-bottom 1px dotted
            &.active
                >a
                    color #fff
                    background #52aab3
                    border 0 none !important
                    span
                        border-bottom-color transparent
    &.hidden-sm
        display inline-block !important

    .livesearch
        width 100% !important
        
