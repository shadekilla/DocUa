.finder-map
    width 100%
    max-width 553px
    position absolute
    left 50%
    margin-left -276px
    z-index 101
    form
        padding: 19px 22px 19px 149px 
        position: relative
        width: 100%
        @media screen560
            padding 20px 18px 21px 19px
        fieldset
            position static

    .finder
        &__search-title
            width 16%
            display: none
        &__field
            width 100%
            margin 0
            position relative
            @media screen560
                padding 0 37px 0 12px 
            &:before
                @media screen560
                    width 36px
                    top 8px
                    height 25px
                    right 0
            input.form-control
                @media screen560
                    height: 38px
                    display: block
                    font-size: 14px
                    padding-right: 0
            &-icon-doctors
                > span
                    top: 11px
                    right: 9px
                    width: 18px
                    height: 21px
                    background: url("i/icons/finder-doc.png") no-repeat center center 
            &-icon-clinics
                > span
                    right 3px
                    top 7px
        &__submit
            width: 114px
            position: absolute
            top: 19px
            left: 21px
            @media screen560
                position: static
                padding: 16px 0 0  
                width: 100%
            .finder__btn-map
                background-color: green_base
                border-radius: 3px
                width: 114px
                height: 39px
                // height 50px
                font-size: 16px
                border: none
                line-height: 39px
                text-shadow: none
                // padding 5px
                // padding: 5px 18px 5px 14px 
                position: relative 
                @media screen560
                    width: 100%
                &:hover
                    background: darken(green_base, 10%) 
                &:active, focus
                    background: darken(green_base, 20%) 
                &:before
                    content: "" 
                    // position: absolute
                    // top: 13px
                    // left: 14px
                    margin: 13px 10px 0 0
                    display: inline-block
                    vertical-align: top 
                    width: 13px
                    height: 13px
                    background: url("i/icons/circle-left.png") no-repeat center center 
                    

    // @media only screen and (max-width: 1400px)
    //     left 7.5%
    //     margin-left 0
    @media only screen and (max-width: 992px)
        // left: 22px
        // right: 22px
        width: auto 
        margin-left: -200px
    @media screen560
        max-width: 285px
        right: auto
        left: 50%
        width: 100%
        margin-left -142px

.finder_inner-pages
    background green_texture 
    .wrap
        margin 0 -10px 
    .finder
        margin 20px 0 
        @media phone
            margin 20px 0 30px 0
        &__search-title
            height 50px 
            line-height 50px
            margin-right 3% 
            padding 0  
            background none
            font-size 20px
            text-transform none 
            &:after
                content none
    form 
        width 100% 
        padding 0 
        &:before
            content none

.subject-scroll
    .jspContainer
        .jspPane
            width 100% !important
        .jspTrack, .jspDrag
            background-color: #d2e1e4
        .jspVerticalBar 
            right 5px

.no-results-finder
    display none
    position absolute
    top 100%
    left 0
    right 0
    width 100%
    border-top-color: transparent
    background-color: #fff
    z-index: 2
    padding: 5px 15px 6px 15px 
    color: #7c888a
    font-size: 15px
    border: 1px solid #d9ecf0
    border-radius: 0 0 2px 2px
    box-shadow: 0 3px 8px rgba(0,0,0,.02)
    line-height: 42px
    &.is-hide
        display block
.helper-autocomplete
    float left
    color: #e8e8e8
    font-size: 12px
    font-weight: 400
    line-height: 17px
    overflow: hidden
    text-overflow: ellipsis
    white-space: nowrap
    width 100%
    @media phone
        display none
    &:hover
        cursor default 
    span
        text-decoration: underline
    a
        color #e8e8e8
        // span
        //     text-decoration underline
            