.med-library
	position relative 
	background #fff
	margin-bottom 20px
	padding 38px
	box-shadow 0 2px 4px rgba(0,64,76,.15)
	border-radius 3px
	&:after
		content ""
		display table
		clear both 
	&__item
		margin-right 35px 
		padding 15px 0
		border-top 1px solid #d9ecf0

.header-styled
	&.header-styled_notopmargin
		margin-top 0 
		.h1
			color text_color 
