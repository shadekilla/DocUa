.share-links
    clearfix()
    margin-bottom 25px
    li
        float left
        &.text
            margin-right 20px
            color text_blue
            font-size 18px
            line-height 34px
        a
            display inline-block
            margin-right 10px
            &.fb
                sprite($share_fb)
            &.tw
                sprite($share_tw)
            &.gp
                sprite($share_gp)