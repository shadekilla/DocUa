.search-result
  .livesearch.top-list
    position absolute
    right 0
    top 36px
    z-index 9
    background-color #fff
    padding 10px 10px 0 10px
    box-shadow 0 1px 5px 0 rgba(11,36,49,0.38)
    width 388px

  .livesearch li:last-child
    padding-bottom 7px

  .more-toggle
    &__link
      top -3px
      padding-bottom 4px
      &:hover
        text-decoration underline
      .more_link
        position relative
        top -1px

  .specialty.article-list-item__date
    margin-left 50px